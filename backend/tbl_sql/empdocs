-- Table: public.employee_docs

-- DROP TABLE IF EXISTS public.employee_docs;

CREATE TABLE IF NOT EXISTS public.employee_docs
(
    doc_id integer NOT NULL GENERATED BY DEFAULT AS IDENTITY ( INCREMENT 1 START 1 MINVALUE 1 MAXVALUE 2147483647 CACHE 1 ),
    emp_id character varying(20) COLLATE pg_catalog."default" NOT NULL,
    profile_photo character varying(200) COLLATE pg_catalog."default",
    aadhar_number character varying(200) COLLATE pg_catalog."default" NOT NULL,
    aadhar_doc character varying(200) COLLATE pg_catalog."default",
    pan_number character varying(200) COLLATE pg_catalog."default" NOT NULL,
    pan_doc character varying(200) COLLATE pg_catalog."default",
    resident_doc character varying(200) COLLATE pg_catalog."default",
    bank_doc character varying(200) COLLATE pg_catalog."default",
    bank_acc_number character varying(200) COLLATE pg_catalog."default" NOT NULL,
    ifsc_code character varying(200) COLLATE pg_catalog."default" NOT NULL,
    bank_branch character varying(200) COLLATE pg_catalog."default" NOT NULL,
    bank_name character varying(200) COLLATE pg_catalog."default" NOT NULL,
    education_doc character varying(200) COLLATE pg_catalog."default",
    exp_cer_doc character varying(200) COLLATE pg_catalog."default",
    sal_slip_doc character varying(200) COLLATE pg_catalog."default",
    createdat timestamp with time zone DEFAULT '2023-09-14 14:04:55.382+05:30'::timestamp with time zone,
    updatedat timestamp with time zone DEFAULT '2023-09-14 14:04:55.382+05:30'::timestamp with time zone,
    "createdAt" timestamp with time zone NOT NULL DEFAULT CURRENT_TIMESTAMP,
    "updatedAt" timestamp with time zone NOT NULL DEFAULT CURRENT_TIMESTAMP,
    CONSTRAINT employee_docs_pkey PRIMARY KEY (doc_id),
    CONSTRAINT employee_docs_emp_id_key UNIQUE (emp_id)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS public.employee_docs
    OWNER to postgres;